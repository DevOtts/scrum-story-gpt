{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "jira-story-gpt": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {          
          "builder": "@angular-devkit/build-angular:browser",
          "options": {            
            "outputPath": "dist/jira-story-gpt",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              {
                "glob": "**/*",
                "input": "extension",
                "output": "./"
              },
              {
                "glob": "icon16.png",
                "input": "src/chrome-extension/assets",
                "output": "/assets"
              },
              {
                "glob": "icon32.png",
                "input": "src/chrome-extension/assets",
                "output": "/assets"
              },              
              {
                "glob": "icon48.png",
                "input": "src/chrome-extension/assets",
                "output": "/assets"
              },
              {
                "glob": "icon128.png",
                "input": "src/chrome-extension/assets",
                "output": "/assets"
              },
              {
                "glob": "jquery.3.7.0.min.js",
                "input": "src/chrome-extension/assets",
                "output": "/assets"
              },
              {
                "glob": "jquery.sendkeys.js",
                "input": "src/chrome-extension/assets",
                "output": "/assets"
              },
              {
                "glob": "bililiteRange.js",
                "input": "src/chrome-extension/assets",
                "output": "/assets"
              },
              {
                "glob": "manifest.json",
                "input": "src/chrome-extension",
                "output": "/"
              },
              {
                "glob": "global-config.json",
                "input": "src/chrome-extension",
                "output": "/"
              },
              {
                "glob": "popup.js",
                "input": "src/chrome-extension",
                "output": "/"
              },
              {
                "glob": "content-script.js",
                "input": "src/chrome-extension",
                "output": "/"
              },
              {
                "glob": "jira.js",
                "input": "src/chrome-extension",
                "output": "/"
              },
              {
                "glob": "trello.js",
                "input": "src/chrome-extension",
                "output": "/"
              },
              {
                "glob": "background.js",
                "input": "src/chrome-extension",
                "output": "/"
              },
              {
                "glob": "style.css",
                "input": "src/chrome-extension",
                "output": "/"
              }
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "jira-story-gpt:build:production"
            },
            "development": {
              "browserTarget": "jira-story-gpt:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "jira-story-gpt:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}
